    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch via NPM",
            "request": "launch",
            "runtimeArgs": [
                "start"
            ],
            "args": [
                "golden-onepod-low-throughput"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Firelens Datajet Endpoint",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/dist/app.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "preLaunchTask": "npm: build",
            "outputCapture": "std"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Firelens Datajet Local",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/dist/app.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "preLaunchTask": "npm: build",
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Suite Launch",
            "runtimeArgs": [
                "-r",
                "ts-node/register"
            ],
            "args": [
                "app.ts",
                "golden"
            ],
            "sourceMaps": true
        }
    ]
}