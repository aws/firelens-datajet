{
	"component": "wrap",
	"library": [
		{
			"referenceId": "stageGenerator",
			"component": "generator",
			"generator": {
				"name": "color-logger",
				"config": {
					"payloadSize": 70000,
					"disableSignal": true
				}
			}
		}
	],
	"config": {
		"wrapper": {
			"name": "fluent-bit-executor",
			"config": {
				"fluentConfigFile":	"./examples/fluent-config/fluent-bit-cloudwatch.conf",
				"grace": 10
			}
		}
	},
	"child": {
		"component": "synchronizer",
		"config": {
			"repeat": 1,
			"waitBefore": 0.5,
			"waitAfter": 0.5,
			"waitBetween": 0.01,
			"isAsync": false
		},
		"children": [
			{
				"generator": {
					"name": "reference",
					"config": {
						"ref": "stageGenerator"
					}
				},
				"datajet": {
					"name": "firelens",
					"config": {
						"logStream": "stdout"
					}
				},
				"stage": {
					"batchRate": 200,
					"batchLimit": 200
				}
			}
		]
	}
}